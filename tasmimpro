      <!-- Case Study -->
      <section id="case-study" class="py-16 bg-white">
        <div class="max-w-6xl mx-auto">
          <h2 class="font-display text-4xl font-bold text-gray-800 mb-12 text-center">
            Real Misol: DebtBusters Kanali
          </h2>

